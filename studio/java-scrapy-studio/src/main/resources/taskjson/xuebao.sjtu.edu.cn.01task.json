[{
  "start": {
    "title": "上海交通大学学报",
    "url": "http://xuebao.sjtu.edu.cn/CN/1006-2467/home.shtml"
  },
  "steps": [
    {
      "pageType": "detailed_list",
      "test": true,
      "captureRules": [
        {
          "property": "title",
          "matchRule": "//td/a[@class='txt_biaoti']",
          "filters": [
            {
              "alias": "trim"
            }
          ]
        }
      ],
      "listMatch": "//div[@id='dqml']//div[contains(@id,'art')]",
      "order": 0,
      "openDetailedMatch": {
        "matchRule": "//td/a[@class='txt_biaoti']",
        "attribute": "href"
      }
    },
    {
      "pageType": "detailed",
      "captureRules": [
        {
          "property": "enTitle",
          "matchRule": "//h3[@class='abs-tit'][2]"
        },
        {
          "property": "doi",
          "matchRule": "//span[@class='doi-doi']/a"
        },
        {
          "property": "cnAabstract",
          "matchRule": "//p[@id='C1']"
        },
        {
          "property": "keyWords",
          "matchRule": "//form/p/a[contains(@onclick,'searchKeyword')]"
        },
        {
          "property": "enAbstract",
          "matchRule": "//p[@id='C2']"
        },
        {
          "property": "enKeyWords",
          "matchRule": "//form/p/a[contains(@onclick,'searchEnKeyword')]"
        },
        {
          "property": "pdfUrl",
          "matchRule": "//meta[@name='citation_pdf_url']",
          "attribute": "content"
        }
      ],
      "order": 1
    }
  ]
}]