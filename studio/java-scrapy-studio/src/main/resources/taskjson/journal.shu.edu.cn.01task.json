[{
  "start": {
    "title": "上海大学学报（自然科学版）",
    "url": "https://www.journal.shu.edu.cn/CN/1007-2861/home.shtml"
  },
  "steps": [
    {
      "pageType": "detailed_list",
      "captureRules": [
        {
          "property": "title",
          "matchRule": "//dd/a[@class='biaoti']",
          "filters": [
            {
              "alias": "trim"
            }
          ]
        },
        {
          "property": "authors",
          "matchRule": "//dd[@class='zuozhe']"
        }
      ],
      "listMatch": "//div[@class='dqml_qbwz']",
      "order": 0,
      "openDetailedMatch": {
        "matchRule": "//dd/a[@class='biaoti']",
        "attribute": "href"
      }
    },
    {
      "pageType": "detailed",
      "captureRules": [
        {
          "property": "enTitle",
          "matchRule": "//h3[@class='abs-tit'][2]"
        },
        {
          "property": "doi",
          "matchRule": "//span[@class='doi-doi']/a"
        },
        {
          "property": "enTitle",
          "matchRule": "//h3[@class='abs-tit'][2]"
        },
        {
          "property": "cnAabstract",
          "matchRule": "//p[@id='C1']"
        },
        {
          "property": "keyWords",
          "matchRule": "//form/p/a[contains(@onclick,'searchKeyword')]"
        },
        {
          "property": "enAbstract",
          "matchRule": "//p[@id='C2']"
        },
        {
          "property": "enKeyWords",
          "matchRule": "//form/p/a[contains(@onclick,'searchEnKeyword')]"
        },
        {
          "property": "pdfUrl",
          "matchRule": "//meta[@name='citation_pdf_url']",
          "attribute": "content"
        }
      ],
      "order": 1
    }
  ]
}]